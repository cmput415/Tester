procedure main() returns integer {
    integer x = 5;

    format(1) -> std_output;
    format('\n') -> std_output;

    format(2147483647) -> std_output;
    format('\n') -> std_output;

    format(10 + 10) -> std_output;
    format('\n') -> std_output;

    format(x) -> std_output;
    format('\n') -> std_output;

    return 0;
}
CHECK:1
CHECK:2147483647
CHECK:20
CHECK:5
