procedure main() returns integer {
    real x = 42.0;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 42.;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 4.2;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 0.42;
    format(x) -> std_output;
    '\n' -> std_output;

    x = .42;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 4.2e-1;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 4.2e+9;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 4.2E5;
    format(x) -> std_output;
    '\n' -> std_output;

    x = .42e-7;
    format(x) -> std_output;
    '\n' -> std_output;

    x = 42E6;
    format(x) -> std_output;
    '\n' -> std_output;

    return 0;
}
CHECK:42
CHECK:42
CHECK:4.2
CHECK:0.42
CHECK:0.42
CHECK:0.42
CHECK:4.2e+09
CHECK:420000
CHECK:4.2e-08
CHECK:4.2e+07
